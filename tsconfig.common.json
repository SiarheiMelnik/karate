{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "preserveConstEnums": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "sourceMap": true,
    "lib": [
      "es2017"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
